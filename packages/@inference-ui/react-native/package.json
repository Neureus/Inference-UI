    "expo-linear-gradient": "~15.0.7",
    "expo-status-bar": "~3.0.8"
  },
  "peerDependencies": {
    "react": "19.2.0",
    "react-native": "0.82.0"
  },
  "devDependencies": {
    "@types/react": "~19.2.2",
    "typescript": "^5.8.2"
