{
  "name": "@inference-ui/react-native",
  "version": "1.0.0",
  "description": "All-in-one React Native package with glassmorphism design system, AI components, events, flows, and hybrid AI engine",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./primitives": {
      "types": "./dist/primitives/index.d.ts",
      "import": "./dist/primitives/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js"
    },
    "./events": {
      "types": "./dist/events/index.d.ts",
      "import": "./dist/events/index.js"
    },
    "./flows": {
      "types": "./dist/flows/index.d.ts",
      "import": "./dist/flows/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest --passWithNoTests",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "start": "expo start",
    "ios": "expo start --ios",
    "android": "expo start --android",
    "web": "expo start --web"
  },
  "keywords": [
    "react-native",
    "expo",
    "glassmorphism",
    "design-system",
    "ui",
    "components",
    "inference-ui",
    "ai",
    "events",
    "analytics",
    "flows",
    "tree-shaking"
  ],
  "author": "Inference UI Team",
  "license": "MIT",
  "dependencies": {
    "expo": "~54.0.13",
    "expo-blur": "~15.0.7",
    "expo-glass-effect": "~0.1.4",
    "expo-linear-gradient": "~15.0.7",
    "expo-status-bar": "~3.0.8"
  },
  "peerDependencies": {
    "react": "19.2.0",
    "react-native": "0.82.0"
  },
  "devDependencies": {
    "@types/react": "~19.2.2",
    "typescript": "^5.8.2"
  }
}
