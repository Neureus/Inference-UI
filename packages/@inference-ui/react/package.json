{
  "name": "@inference-ui/react",
  "version": "1.0.0",
  "description": "All-in-one React package for AI streaming, events, flows, and generative UI with tree-shaking support",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "import": "./dist/hooks/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js"
    },
    "./events": {
      "types": "./dist/events/index.d.ts",
      "import": "./dist/events/index.js"
    },
    "./flows": {
      "types": "./dist/flows/index.d.ts",
      "import": "./dist/flows/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest --passWithNoTests",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "ai",
    "streaming",
    "chat",
    "completion",
    "generative-ui",
    "inference",
    "llm",
    "gpt",
    "openai",
    "anthropic",
    "cloudflare",
    "workers-ai",
    "vercel-ai",
    "events",
    "analytics",
    "flows",
    "tree-shaking"
  ],
  "author": "Inference UI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Neureus/Inference-UI.git",
    "directory": "packages/@inference-ui/react"
  },
  "homepage": "https://inference-ui.dev",
  "bugs": {
    "url": "https://github.com/Neureus/Inference-UI/issues"
  },
  "dependencies": {
    "zod": "^3.24.1"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "typescript": "^5.8.2",
    "react": "^19.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
