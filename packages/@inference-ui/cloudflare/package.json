{
  "name": "@inference-ui/cloudflare",
  "version": "0.1.0",
  "description": "Cloudflare Workers, D1, R2, and Workers AI integration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:streaming": "tsc --project tsconfig.streaming.json",
    "build:personql": "tsc --project tsconfig.personql.json",
    "build:inference-service": "tsc --project tsconfig.inference-service.json",
    "build:all": "npm run build && npm run build:streaming && npm run build:personql && npm run build:inference-service",
    "dev": "wrangler dev",
    "dev:streaming": "wrangler dev --config wrangler-streaming.toml",
    "dev:personql": "wrangler dev --config wrangler-personql.toml",
    "dev:inference-service": "wrangler dev --config wrangler-inference-service.toml",
    "deploy": "wrangler deploy",
    "deploy:streaming": "wrangler deploy --config wrangler-streaming.toml",
    "deploy:personql": "wrangler deploy --config wrangler-personql.toml",
    "deploy:inference-service": "wrangler deploy --config wrangler-inference-service.toml",
    "deploy:all": "npm run deploy:inference-service && npm run deploy:personql && npm run deploy:streaming && npm run deploy",
    "test": "jest --passWithNoTests",
    "lint": "eslint src src-streaming src-personql src-inference-service --ext .ts,.tsx",
    "typecheck": "tsc --noEmit && tsc --noEmit --project tsconfig.streaming.json && tsc --noEmit --project tsconfig.personql.json && tsc --noEmit --project tsconfig.inference-service.json"
  },
  "keywords": ["cloudflare", "workers", "d1", "r2", "workers-ai"],
  "author": "Liquid UI Team",
  "license": "MIT",
  "dependencies": {
    "@inference-ui/api": "^0.1.0",
    "@inference-ui/core": "^0.1.0",
    "graphql": "^16.9.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250115.0",
    "wrangler": "^4.43.0",
    "typescript": "^5.8.2"
  }
}
